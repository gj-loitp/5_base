<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rootView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/themeBackgroundColor">

    <com.loitp.views.ab.LActionBar
        android:id="@+id/lActionBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent" />

    <com.factor.bouncy.BouncyNestedScrollView
        android:id="@+id/scrollView"
        style="@style/ScrollVertical"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lActionBar">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="@dimen/margin_padding_medium"
            android:paddingBottom="@dimen/margin_padding_medium">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btActionBar"
                style="@style/ButtonMenu"
                android:text="@string/action_bar" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btAndroidRibbon"
                style="@style/ButtonMenu"
                android:text="AndroidRibbon" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btAnswerView"
                style="@style/ButtonMenu"
                android:text="@string/answerview" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btBottomBarBlur"
                style="@style/ButtonMenu"
                android:text="@string/bottom_navigation_bar" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btBottomSheet"
                style="@style/ButtonMenu"
                android:text="@string/bottom_sheet" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btButton"
                style="@style/ButtonMenu"
                android:text="@string/button" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btCalendar"
                style="@style/ButtonMenu"
                android:text="@string/calendar" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btCardView"
                style="@style/ButtonMenu"
                android:text="Card View" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btCodeView"
                style="@style/ButtonMenu"
                android:text="@string/codeview" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btCodeViewKbiakov"
                style="@style/ButtonMenu"
                android:text="CodeView-Android" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btCornerSheet"
                style="@style/ButtonMenu"
                android:text="@string/corner_sheet" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btDialog"
                style="@style/ButtonMenu"
                android:text="@string/dialog" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btDraggableFlipView"
                style="@style/ButtonMenu"
                android:text="@string/draggable_flipview" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btDragView"
                style="@style/ButtonMenu"
                android:text="@string/drag_view" />

