            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btEditText"
                style="@style/ButtonMenu"
                android:text="@string/edittext" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btFbCmt"
                style="@style/ButtonMenu"
                android:text="@string/facebook_comment" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btFingerPaintView"
                style="@style/ButtonMenu"
                android:text="@string/finger_paint_view" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btMenuGraphViewActivity"
                style="@style/ButtonMenu"
                android:text="@string/menu_graph_view_activity" />

            <androidx.appcompat.widget.AppCompatButton
