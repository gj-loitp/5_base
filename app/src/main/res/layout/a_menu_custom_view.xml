                android:text="@string/seek_bar" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btSimpleRatingBar"
                style="@style/ButtonMenu"
                android:text="@string/simple_rating_bar" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btSpotlight"
                style="@style/ButtonMenu"
