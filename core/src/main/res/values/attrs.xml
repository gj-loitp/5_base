    <declare-styleable name="PorterImageView">
        <attr name="siShape" format="integer|reference" />
    </declare-styleable>
    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension" />
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension" />
        <attr name="indicatorName" format="string" />
        <attr name="indicatorColor" format="color" />
    </declare-styleable>
    <declare-styleable name="LColorSwitchCompat">
        <attr name="toggleOnColor" format="color" />
        <attr name="toggleOffColor" format="color" />
        <attr name="bgOnColor" format="color" />
        <attr name="bgOffColor" format="color" />
    </declare-styleable>
    <declare-styleable name="LCircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
    <declare-styleable name="ZoomEngine">
        <attr name="hasClickableChildren" format="boolean" />
        <attr name="overScrollable" format="boolean" />
        <attr name="overScrollHorizontal" format="boolean" />
        <attr name="overScrollVertical" format="boolean" />
        <attr name="overPinchable" format="boolean" />
        <attr name="minZoom" format="float" />
        <attr name="maxZoom" format="float" />
        <attr name="minZoomType" format="enum">
            <enum name="zoom" value="0" />
            <enum name="realZoom" value="1" />
        </attr>
        <attr name="maxZoomType" format="enum">
            <enum name="zoom" value="0" />
            <enum name="realZoom" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundedImage">
        <attr name="rounded_radius" format="float" />
    </declare-styleable>
    <declare-styleable name="CircleImage">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="add_shadow" format="boolean" />
        <attr name="shadow_color" format="color" />
        <attr name="shadow_radius" format="float" />
    </declare-styleable>
    <declare-styleable name="ColorTextView">
        <attr name="color_texts" format="string" />
        <attr name="color_arrays" format="string" />
    </declare-styleable>
    <declare-styleable name="MultiScrollNumber">
        <attr name="primary_number" format="integer" />
        <attr name="target_number" format="integer" />
        <attr name="number_size" format="integer" />
        <!--<attr name="number_color" format="color"/>-->
    </declare-styleable>
    <declare-styleable name="VerticalMarqueeTextView">
        <attr name="autoStartMarquee" format="boolean" />
        <attr name="marqueeSpeed" format="reference|integer" />
        <attr name="text" format="reference|string" />
        <attr name="textSize" format="reference|dimension" />
        <attr name="textColor" format="reference|color" />
        <attr name="textAppearance" format="reference" />
        <attr name="textStyle">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
        </attr>
        <attr name="typeface">
            <enum name="normal" value="0" />
            <enum name="sans" value="1" />
            <enum name="serif" value="2" />
            <enum name="monospace" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="TranslucentView">
        <attr name="translucentBackgroundColor" format="color" />
    </declare-styleable>
    <declare-styleable name="FButton">
        <attr name="shadowEnabled" format="boolean" />
        <attr name="buttonColor" format="color" />
        <attr name="shadowColor" format="color" />
        <attr name="shadowHeight" format="dimension" />
        <attr name="cornerRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MaterialTextField">
        <attr name="mtf_cardCollapsedHeight" format="dimension" />
        <attr name="mtf_labelColor" format="color" />
        <attr name="mtf_image" format="reference" />
        <attr name="mtf_hasFocus" format="boolean" />
        <attr name="mtf_animationDuration" format="integer" />
        <attr name="mtf_openKeyboardOnFocus" format="boolean" />
        <attr name="mtf_backgroundColor" format="color" />
    </declare-styleable>
    <declare-styleable name="BiuEditStyle">
        <attr name="biu_text_color" format="color" />
        <attr name="biu_text_start_size" format="dimension" />
        <attr name="biu_text_scale" format="float" />
        <attr name="biu_duration" format="integer" />
        <attr name="biu_type" format="enum">
            <enum name="flyup" value="0" />
            <enum name="flydown" value="1" />
        </attr>

    </declare-styleable>
    <declare-styleable name="CustomToggle">
        <attr name="ctg_addLeftIcon" format="reference" type="drawable" />
        <attr name="ctg_addRightIcon" format="reference" />
        <attr name="ctg_addSlideBackgroundColor" format="color" />
        <attr name="ctg_addSlideColor" format="color" />
        <attr name="ctg_addMagnification" format="integer" />
        <attr name="ctg_addAnimationTime" format="integer" />
        <attr name="ctg_addAnimationType" format="enum">
            <enum name="jack_animation" value="0" />
            <enum name="phantom_animation" value="1" />
            <enum name="extra_animation" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CircularProgressBar">
        <attr name="maximum" format="float" />
        <attr name="progress" format="float" />
        <attr name="animateProgress" format="boolean" />
        <attr name="progressAnimationDuration" format="integer" />
        <attr name="foregroundStrokeColor" format="color" />
        <attr name="foregroundStrokeWidth" format="dimension" />
        <attr name="drawBackgroundStroke" format="boolean" />
        <attr name="backgroundStrokeColor" format="color" />
        <attr name="backgroundStrokeWidth" format="dimension" />
        <attr name="startAngle" format="float" />
        <attr name="indeterminate" format="boolean" />
        <attr name="indeterminateMinimumAngle" format="float" />
        <attr name="indeterminateArcAnimationDuration" format="integer" />
        <attr name="indeterminateRotationAnimationDuration" format="integer" />
    </declare-styleable>
    <declare-styleable name="RealtimeBlurView">
        <attr name="realtimeBlurRadius" format="dimension" />
        <attr name="realtimeDownsampleFactor" format="float" />
        <attr name="realtimeOverlayColor" format="color" />
    </declare-styleable>
    <declare-styleable name="AutoViewPager">
        <attr name="avp_indeterminate" format="boolean" />
        <attr name="avp_autoScroll" format="boolean" />
        <attr name="avp_duration" format="integer" />
    </declare-styleable>
    <declare-styleable name="ShapeFlyer">
        <attr name="shape_width" format="dimension" />
        <attr name="shape_height" format="dimension" />
        <attr name="enable_alpha" format="boolean" />
        <attr name="enable_scale" format="boolean" />
        <attr name="from_scale" format="float" />
        <attr name="to_scale" format="float" />
        <attr name="from_alpha" format="float" />
        <attr name="to_alpha" format="float" />
    </declare-styleable>
    <declare-styleable name="ParallaxImageView">
        <attr name="parallax_ratio" format="float" />
        <attr name="center_crop" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CircularSeekBar">
        <attr name="enabled" format="boolean" />
        <attr name="showIndicator" format="boolean" />
        <attr name="min" format="float" />
        <attr name="max" format="float" />
        <attr name="speedMultiplier" format="float" />
        <attr name="progressCircularSeekBar" format="float" />
        <attr name="showProgressText" format="boolean" />
        <attr name="ringWidth" format="float" />
        <attr name="progressText" format="string" />
        <attr name="showInnerCircle" format="boolean" />
        <attr name="ringColor" format="color" />
        <attr name="innerCircleColor" format="color" />
        <attr name="progressTextColor" format="color" />
        <attr name="progressTextSize" format="float" />
    </declare-styleable>
    <declare-styleable name="AutofitEditText">
        <!-- Minimum size of the text. -->
        <attr name="minTextSize" format="dimension" />
        <!-- Amount of precision used to calculate the correct text size to fit within its
        bounds. Lower precision is more precise and takes more time. -->
        <attr name="precision" format="float" />
        <!-- Defines whether to automatically resize text to fit to the view's bounds. -->
        <attr name="sizeToFit" format="boolean" />
    </declare-styleable>
    <declare-styleable name="AutofitTextView">
        <!-- Minimum size of the text. -->
        <attr name="minTextSizeTv" format="dimension" />
        <!-- Amount of precision used to calculate the correct text size to fit within its
        bounds. Lower precision is more precise and takes more time. -->
        <attr name="precisionTv" format="float" />
        <!-- Defines whether to automatically resize text to fit to the view's bounds. -->
        <attr name="sizeToFitTv" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ArcDrawer">
        <attr name="arc_width" format="dimension" />
        <attr name="arc_cropDirection" format="enum">
            <enum name="cropInside" value="0" />
            <enum name="cropOutside" value="1" />
        </attr>
    </declare-styleable>
    <!--LoadingButton-->
    <declare-styleable name="buttonLoading">

        <attr name="BL_enable" format="boolean" />
        <attr name="BL_idParent" format="integer" />
        <attr name="BL_font" format="string" />
        <attr name="BL_text" format="string" />
        <attr name="BL_textColor" format="color" />
        <attr name="BL_backgroundColor" format="color" />
        <attr name="BL_circleColor" format="color" />
        <attr name="BL_circleColorSecond" format="color" />
        <attr name="BL_backgroundDisableColor" format="color" />
        <attr name="BL_textSize" format="dimension" />
        <attr name="BL_stateShow" format="integer">
            <enum name="normal" value="1" />
            <enum name="animationStart" value="2" />
            <enum name="progress" value="3" />
            <enum name="animationFinish" value="4" />
        </attr>

    </declare-styleable>
    <declare-styleable name="ContinuousScrollableImageView">
        <attr name="imageSrc" format="reference" />
        <attr name="direction" />
        <attr name="duration" format="integer" />
        <attr name="scaleType" />
    </declare-styleable>

    <attr name="direction">
        <enum name="up" value="0" />
        <enum name="right" value="1" />
        <enum name="down" value="2" />
        <enum name="left" value="3" />
    </attr>

    <attr name="scaleType">
        <enum name="matrix" value="0" />
        <enum name="fitXy" value="1" />
        <enum name="fitStart" value="2" />
        <enum name="fitCenter" value="3" />
        <enum name="fitEnd" value="4" />
        <enum name="center" value="5" />
        <enum name="centerCrop" value="6" />
        <enum name="centerInside" value="7" />
    </attr>

    <declare-styleable name="PanoramaImageView">
        <attr name="piv_enablePanoramaMode" format="boolean" />
        <attr name="piv_invertScrollDirection" format="boolean" />
        <attr name="piv_show_scrollbar" format="boolean" />
    </declare-styleable>

    <declare-styleable name="draggable_panel">
