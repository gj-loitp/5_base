<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RecyclerViewPager">
        <attr name="rvp_triggerOffset" format="float" />
        <attr name="rvp_flingFactor" format="float" />
        <attr name="rvp_singlePageFling" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LSProgressBar">
        <attr name="filterColor" format="color" />
    </declare-styleable>

    <declare-styleable name="GridCenterView">
        <attr name="dividerHeight" format="dimension" />
        <attr name="dividerColor" format="reference" />
        <attr name="spanNumber" format="integer" />
    </declare-styleable>
    <declare-styleable name="ParallaxViewPager">
        <attr name="mode">
            <enum name="left_overlay" value="0" />
            <enum name="right_overlay" value="1" />
            <enum name="none" value="2" />
        </attr>
        <attr name="outset" format="fraction|dimension" />
        <attr name="interpolator" format="reference" />
        <attr name="shadow_width" format="dimension" />
        <attr name="left_shadow" format="reference" />
        <attr name="right_shadow" format="reference" />
    </declare-styleable>
    <declare-styleable name="LabelsView">
        <attr name="max_labels" format="integer" />
        <attr name="show_cross" format="boolean" />
        <attr name="icon_cross" format="reference" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
        <attr name="label_background_res" format="reference" />
        <attr name="label_clickable" format="boolean" />
        <attr name="label_padding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ShineButton">
        <attr name="btn_color" format="color" />
        <attr name="btn_fill_color" format="color" />
        <attr name="shine_count" format="integer" />
        <attr name="shine_turn_angle" format="float" />
        <attr name="small_shine_offset_angle" format="float" />
        <attr name="enable_flashing" format="boolean" />
        <attr name="allow_random_color" format="boolean" />
        <attr name="small_shine_color" format="color" />
        <attr name="big_shine_color" format="color" />
        <attr name="shine_animation_duration" format="integer" />
        <attr name="click_animation_duration" format="integer" />
        <attr name="shine_distance_multiple" format="float" />
        <attr name="shine_size" format="dimension" />
